{
	"compilerOptions": {
		"noEmit": true,
		"strict": true,
		"newLine": "lf",
		"declaration": true,
		"importHelpers": false,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"sourceMap": false,
		"stripInternal": true,
		"downlevelIteration": true,
		"rootDir": "src",
		"outDir": "build",
	},
	"references": [
		{ "path": "src/page" },
		{ "path": "src/runner" },
		{ "path": "src/scripts" }
	]
}
