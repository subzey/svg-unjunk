{
	"compilerOptions": {
		"declaration": true,
		"importHelpers": true,
		"lib": [
			"es2017",
		],
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"sourceMap": false,
		"strict": true,
		"strictFunctionTypes": true,
		"stripInternal": true,
		"downlevelIteration": true,
        "target": "es2017",
        "module": "commonjs"
	}
}
